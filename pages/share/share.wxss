/* pages/share/share.wxss */
/*分享*/
.myvip-content{
	    padding: 70% 15px 80px 15px !important;
}
.share-img {
    width:52%;
    height:52%;
    margin:0 auto;
    text-align:center;
    margin-top:12px;
}
.share-content{
	text-align: center;
}
.tou{
	width: 60px;
	height: 60px;
	border-radius: 30px;
	margin: 0 auto;
	border: 3px solid #fff;
}
.tou image{
	width: 100%;
	height: 100%;
    border-radius:50%;
}
.share-content p{
	line-height: 50px;
	color: #ff353f;
	text-align: center;
	font-size: 16px;
}
.share-title {
    text-align: center;
    color: #fff;
    position: relative;
}
.share-title {
    padding: 10px 15px 10px 15px;
    font-size: 14px;
    color: #ff4444;
}
.share-title image {
    width:55%;
    margin:0 auto;
    height:13px;
}
.share-title span {
    width: 98px;
    display: inline-block;
    height: 24px;
    line-height: 24px;
    color: #cacaca;
    font-size: 14px;
}
.share-title image.left {
    left: 5%;
}
.share-title image.right {
    right: 5%;
}
.share_vip{
    width:32px;
    height:16px;
    vertical-align:3px;
    margin-right:10px;
    margin-top:30px;
}
.shareBtn {
    position: absolute;
    right: 11px;
    top: 9px;
    width: 28px;
    height: 28px;
    display: block;
    margin: 0 !important;
    background: url("data:image/png;base64,");
    background-size: 100% 100%;
    border: none !important;
    outline: none !important;
    padding:10px;
    border-radius: 50%;
}
button::after {
    border: none;
}
.erweima{
    background-color:#fff;
    border:2px solid #b7ece5;
    width:100px;
    height:100px;
    margin:0 auto;

}
.a_121{
    width:112px;
    margin-top:30px;
    height:23px;
}
.erweima-tu {
    width: 92%;
    vertical-align: middle;
    margin-top: 5px;
    height: 92%
}
.daka{
    background: -webkit-linear-gradient(90deg, #50f1c0, #29ddd9);
    background: -o-linear-gradient(90deg, #50f1c0, #29ddd9);
    background: -moz-linear-gradient(90deg, #50f1c0, #29ddd9);
    background: linear-gradient(90deg, #50f1c0, #29ddd9);
    position: absolute;
    z-index:222;
    width:48px;
    height:48px;
    border-radius: 50%;
    text-align: center;
    margin:0 auto;
    top:10px;
    right:15px;
}
.bg_share{
    width:100%;
    height:250px;
    overflow: hidden;
}
.bg_share image{
    width:100%;
    height:250px;
}
/*分享*/