.passadd{
	margin:0px 30px;
	clear:both;
	height:40px;
	line-height:40px;
	color:#cacaca;
	width:155px;
	overflow:hidden;
	text-overflow:ellipsis;
	white-space:nowrap;
}
.placchecked{
	color: #000!important;
}
.radioGroup{
	position: relative;
    width: 80%;
    margin: 5px auto;
    line-height: 40px;
}
.radio {
	display: inline-block;
	margin-right: 18px;
	color: #181818;
	position: relative;height: 20px;line-height: 20px;text-indent: 25px
}

.radioGroup .radio>input[type="radio"] {
	vertical-align: -3px;
	outline: none;
	-webkit-appearance: none;
	background-image: url(data:image/png;base64,);
	width: 16px;
	height: 16px;
	background-size: 16px 16px;
	margin-right: 5px;
}

.radioGroup .radio>input[type="radio"]:checked {
	width: 16px;
	height: 16px;
	background-image: url(data:image/png;base64,);
	background-size: 100% 100%;
	border: 0;
}


