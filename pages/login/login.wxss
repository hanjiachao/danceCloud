.content{
	padding-top: 100px;
}
.login-box{
	padding-top: 15px;
	width: 84%;
	margin: 0px auto;
	background-color: white;
	border-radius: 16px;
	padding-bottom: 35px;
}
.login-box .row {
	position: relative;
	width: 80%;
	margin: 5px auto;
	border-bottom: 1px solid #ececec;
}
.login-box .row .left {
	width: 18px;
    height: 18px;
    display: block;
    position: absolute;
    top: 12px;
    left: 0px;
}
.content .login-box .row>.a_06 {
    width: 8px;
    height: 13px;
    display: block;
    position: absolute;
    top: 14px;
    right: 2%;
}
.login-box .row>input {
	width: calc(96% - 25px);
	height: 40px;
	line-height: 40px;
	margin-left: calc(4% + 20px)
}

.login-box .row>input::-webkit-input-placeholder {
	color: #cacaca;
}

.login-box .btn {
	width: 71%;
    height: 38px;
    line-height: 38px;
    border-radius: 21px;
    background: -webkit-linear-gradient(90deg, #50f1c0, #29ddd9); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(90deg, #50f1c0, #29ddd9); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(90deg, #50f1c0, #29ddd9); /* Firefox 3.6 - 15 */
    background: linear-gradient(90deg, #50f1c0, #29ddd9); /* 标准的语法（必须放在最后） */
    color: #fff;
    text-align: center;
    margin: 40px auto 0;
    font-size: 16px;
    -webkit-box-shadow:1px 6px 20px #c6f6f5; 
    box-shadow:1px 6px 20px #c6f6f5;
}
.content .row .detail {
    width: calc(96% - 25px);
    height: 40px;
    line-height: 40px;
    margin-left: 0px;
    color: #222;
}


