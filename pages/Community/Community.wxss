/* pages/Community/Community.wxss */
/*社群*/
.release{width: 60px;height: 60px;display: block;position: fixed;right: calc(50% - 30px);bottom: 15px;z-index: 4}
.progress{position: fixed;top: 0;left: 0;width: 100%;z-index: 2;background: -webkit-linear-gradient(90deg, #50f1c0, #29ddd9);background: -o-linear-gradient(90deg, #50f1c0, #29ddd9);background: -moz-linear-gradient(90deg, #50f1c0, #29ddd9);background: linear-gradient(90deg, #50f1c0, #29ddd9);}
.level{float: left;height: 40px;line-height: 40px;width: 60px;text-align: left;font-size: 14px;color: #fff;margin-left: 15px}
.progressTitle{color: #fff;text-align: center;width: 100px;height: 40px;line-height: 40px;font-size: 14px;float: left}
.progressTitle text{color: rgb(255,244,111)}
.bar{width: calc(100% - 260px);height: 10px;background: rgb(122,240,218);border-radius: 5px;float: left;margin-top: 15px;overflow: hidden}
.percent{background: rgb(254,245,110);height: 10px;border-radius: 5px;max-width: 100%}
.btn{float: right;width: 70px;height: 24px;line-height: 24px;text-align: center;margin-top: 8px;background: #fff;border-radius: 12px;color: rgb(40,213,209);margin-right: 15px}
.comment{width: 100%;height: 35px;position: fixed;bottom: 0;left: 0;background: #fff;z-index: 5;border-top: 1px solid #ddd}
.text{width: calc(100% - 90px);padding: 0 10px;height: 35px;line-height: 35px;background: #fff}
.commentBtn{background: rgb(40,213,209);color: #fff;width: 70px;height: 35px;line-height: 35px;position: absolute;right: 0;bottom: 0;text-align: center}
/*video{width: 95% !important;}*/
video{
    position: absolute;
    width:100%;
    height:calc(100% - 40px);
    z-index:111111;
    left:0px;
    top:40px;
}
.play_video{
    position: fixed;
    top:0px;
    left:0px;
    width:100%;
    height:100%;
    z-index:333;
    background: #000;
}
.callback{
    color: #fff;
    margin-left:15px;
    margin-top:15px;
}
.itemContent{width:100%;display:block;word-break:break-all;}

.share-box{
	display: block;
	position: relative;
	height: 10px;
}
.release-tu .li:nth-child(3){
  text-align: right
}
.detail .li:nth-child(2) span:nth-child(2) span{
	float: right
}
.detail .li:nth-child(2) img {
    width: 16px;
    height: 16px;
    margin-right: 10px;
    vertical-align: -3px;
}
.detail .li:nth-child(2) span:nth-child(1) span {
    float: none;
    color: #a2a2a2;
    display: inline-block;
}
.detail .li:nth-child(2) span {
    color: #585858;
}
.detail .li:nth-child(2) span:nth-child(2) {
    color: #a2a2a2;
}
.post-box .li .post-tu image{
    width: 100%;
    height: auto;
}
.post-content{
	margin-bottom: 10px;
	padding-bottom: 10px;
	background-color: white;
	margin-top: 8px;
}
.content-box .title{
	color: #222;
}
.post-comment{
	padding: 50px 15px 10px;
    background-color: white;
    position: relative;
}
.post-comment .li:nth-child(2) span:nth-child(4){
	background-color: #f7f7f7;
	display: block;
}
.comment-box .right:nth-child(2) .name{
	color: #29D5D0;
	margin-bottom: 8px;
	display: block;
}
 .post-comment .replay .li{
	width: 100%;
	background-color: #f9f9f9;
	float: none;
}
.replay .li span{
	color: #222222;
	font-size: 12px;
}
.likeNum{
    color: #29D5D0 !important;
    margin: 0 1px;
}
.replay{
	display: inline-block;
	padding: 5px 10px 5px 10px;
	background-color: #f7f7f7;
	width: calc(100% - 20px);
}
.post-comment .replay  .li {
    margin-left: 0px;
    line-height: 25px;
}
.replay .more{
	font-size: 12px;
	color: #ffa128;
	margin-top: 10px;
}
.post-comment .li span:nth-child(1) .user{
	float: none;
	display: initial;
	color: #29d5d0;
}
.comment-box .left:nth-child(1) {
    float: left;
    width: 50px;
    height: 50px;
}
.comment-box .right:nth-child(2) {
    margin-left: 60px;
    padding-bottom: 15px;
    position: relative;
}
.comment-box li:nth-child(1) .tou {
    width: 100%;
    height: 100%;
    border-radius: 50%;
}
.community{
