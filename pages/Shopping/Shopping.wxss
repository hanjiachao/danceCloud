/* pages/Shopping/Shopping.wxss */
.lunbo{
  width: 100%;
  height: 200px;
  position: relative
}
.bg{
	position: fixed;
	width: 100%;
	height: 100%;
	z-index: -1;
	background-color: white;
}
/*搜索部分*/
.chaxun {
	height: 200px;
    width: 100%;
}

.search_box {
	height:35px;
    background-color:#fff;
    border-radius:40px;
    position:absolute;
    top:15px;
    left:50%;
    line-height:35px;
    width:90%;
    margin-left:-45%;
}

.search_box .left {
	float: left;
	width: 18px;
	line-height: 35px;
	padding-left:10px;
  height: 18px
}

.search_box .left image {
	width: 100%;
	vertical-align: -5px;
  height: 100%
}

.search_box .search {
	line-height: 35px;
	float: right;
	width:55px;
  position: relative;
}
.shu{
	height: 17px;
    width: 1px;
    background-color: #cacaca;
    display: inline-block;
    position: absolute;
    top: 9px;
}
.search_box .search .button {
	color: #29d5d0;
	font-size: 15px;
	background-color: transparent;
	cursor: pointer;
	padding-left: 10px;
}

.search_box span image {
	width: 85%;
	display: inline-block;
	vertical-align: middle;
}

.search_box input {
	color:#d9d9d9;
  line-height:35px;
  width:100%;
  height:35px;
}

.search_box .center {
	margin: 0px 60px 0px 38px;
}
.Collage-menu {
    background: #fff;
    height: 20px;
    line-height: 20px;
    display: flex;
    border-top: 2px solid #f0f0f0;
    position: relative;
    border-bottom: 2px solid #f0f0f0;
    padding: 10px 0px;
}
.Collage-menu .active {
    color: #29d5d0!important;
}
.slide-image{
    width:100%;
    height:200px;
}
.Collage-menu .active .xian {
    width: 40%;
    height: 1px;
    background-color: #29d5d0;
    position: absolute;
    left: 50%;
    margin-left: -20%;
    bottom: -10px;
}
.Collage-menu .li {
    position: relative;
    flex: 1;
    font-size: 15px;
    color: #222;
    border-right: 1px solid #f0f0f0;
    text-align: center;
}
.my-release .order {
    border-bottom: 1px solid #f0f0f0;
    background-color: white;
    margin-bottom: 6px;
    font-size: 14px;
    padding: 10px 15px;
    position: relative;
}
.my-release .ul .li .left {
    float: left;
     width: 80px; 
     display: block
}
.order-tu {
    width: 70px;
    height: 70px;
    margin-right: 15px;
}
.my-release .ul .li .index-right {
    float: left;
    line-height: 40px;
    width: calc(100% - 80px);
}
.my-release .ul .order .right label {
    line-height: 25px;
    height: 25px;
}
.my-release .ul .li .index-right label {
    color: #333;
    display: block
}
.my-release .ul .order .right .two {
    font-size: 12px;
    color: #919191;
}
.my-release .ul .order .right label {
    line-height: 25px;
    height: 25px;
}
.my-release .ul .order .right .two span {
    color: #ff2c5e;
    padding: 0px 3px;
}
.my-release .ul .order .right .three {
    color: #999999;
}
.my-release .ul .order .right label {
    line-height: 25px;
    height: 25px;
}
.my-release .ul .order .right .three span {
    /* font-size: 16px; */
    color: #29D5D0;
}
span {
    display: inline-block;
}
.enter {
    padding: 0px 10px;
    background: -webkit-linear-gradient(90deg, #50f1c0, #29ddd9);
    background: -o-linear-gradient(90deg, #50f1c0, #29ddd9);
    background: -moz-linear-gradient(90deg, #50f1c0, #29ddd9);
    background: linear-gradient(90deg, #50f1c0, #29ddd9);
    border-radius: 12px;
    color: white;
    position: absolute;
    bottom: 13px;
    right: 15px;
}
.my-release .ul .li .left label, .my-release .ul .li .right label {
    display: block;
    line-height: 25px;
}
.my-release .ul .order .right .jiage {
    color: #29D5D0;
}