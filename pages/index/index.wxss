.Collage{
	position: relative;
	border-bottom: 6px solid #fff;
}
.Collage-title p{
	color: #fff;
	line-height: 22px;
	font-size: 14px;
  display: block;
	width:80%;
	overflow:hidden;
	height:22px;
    text-overflow: ellipsis;
	white-space:nowrap
}
.Collage-title p:nth-child(2){
	font-size: 12px;
}
.Collage-title p:nth-child(2) span{
	font-size: 14px;
	color: #61fdcf;
}
.Collage-title{
	background-color: rgba(0,0,0,0.3);
	padding: 6px 15px;
	position: absolute;
	width: calc(100% - 30px);
	bottom: 4px;
}
.enter{
	padding: 2px 10px;
	background: -webkit-linear-gradient(90deg, #50f1c0, #29ddd9); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(90deg, #50f1c0, #29ddd9); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(90deg, #50f1c0, #29ddd9); /* Firefox 3.6 - 15 */
    background: linear-gradient(90deg, #50f1c0, #29ddd9); /* 标准的语法（必须放在最后） */
	border-radius: 12px;
	color: white;
	position: absolute;
	bottom: 13px;
	right: 15px;
}
