/**index.wxss**/
/*公共部分*/

html,
body,
header,
section,
footer,
div,
ul,
ol,
li,
img,
a,
span,
em,
del,
legend,
center,
strong,
var,
fieldset,
form,
label,
dl,
dt,
dd,
cite,
input,
hr,
time,
mark,
code,
figcaption,
figure,
textarea,
h1,
h2,
h3,
h4,
h5,
h6,
p {
	margin: 0;
	border: 0;
	padding: 0;
	font-style: normal;
}

ol,
ul {
	list-style: none;
}

ol,
ul {
	list-style: none;
}

ul li {
	list-style: none;
}

input,
button,
textarea {
	border: 0;
	margin: 0;
	padding: 0;
	outline: none;
}
.music_image{
	width:24px;
	height:24px;
	position:absolute;
	bottom:20px;
	left:20px;
}
.music_image image{
	width:24px;
	height:24px;
}
span {
	display: inline-block;
}

a:active,
a:hover {
	outline: 0;
}

a,
a:visited {
	text-decoration: none;
}

label,
.wordWrap {
	word-wrap: break-word;
	word-break: break-all;
}

table {
	border-collapse: collapse;
	border-spacing: 0;
}

td,
th {
	padding: 0;
}

nav,
article,
aside,
details,
main,
header,
footer,
section,
fieldset,
figcaption,
figure {
	display: block;
}

img,
a,
button,
em,
del,
strong,
var,
label,
cite,
small,
time,
mark,
code,
textarea {
	display: inline-block;
}
.clearfix{
	clear: both;
}
/*公共部分*/
/*首页*/
.lunbo{
  width: 100%;
  height: 200px;
  position: relative
}
.bg{
	position: fixed;
	width: 100%;
	height: 100%;
	z-index: -1;
	background-color: white;
}
/*搜索部分*/
.chaxun {
	height: 200px;
    width: 100%;
	position: relative;
}

.search_box {
	height:35px;
	background-color:#fff;
	border-radius:40px;
	position:absolute;
	top:15px;
	left:50%;
	line-height:35px;
	width:90%;
	margin-left:-45%;
}

.search_box .left {
	float: left;
	width: 18px;
	line-height: 35px;
	padding-left:10px;
  height: 18px
}

.search_box .left image {
	width: 100%;
	vertical-align: -5px;
  height: 100%
}

.search_box .search {
	line-height: 35px;
	float: right;
	width:55px;
  position: relative;
}
.shu{
	height: 17px;
    width: 1px;
    background-color: #cacaca;
    display: inline-block;
    position: absolute;
    top: 9px;
}
.search_box .search .button {
	color: #29d5d0;
	font-size: 15px;
	background-color: transparent;
	cursor: pointer;
	padding-left: 10px;
}

.search_box span image {
	width: 85%;
	display: inline-block;
	vertical-align: middle;
}

.search_box input {
	color:#d9d9d9;
  line-height:35px;
  width:100%;
  height:35px;
}

.search_box .center {
	margin: 0px 60px 0px 38px;
}
