/*搜索*/
.searchPage{
    width: 100%;
    background-image: url(../images/a_43.png);
    background-size: 100% 100%;
    background-repeat: no-repeat;
    padding: 10px 0px;
}
.search_box .cancel-search button {
    font-weight: normal;
    color: #29d5d0;
    font-size: 15px;
    background-color: transparent;
    cursor: pointer;
    padding-left: 10px;
}
.cancel-box input::-webkit-input-placeholder{
	color: #222;
	font-size: 14px;
}
.Collage-menu {
    background: #fff;
    height: 20px;
    line-height: 20px;
    display: flex;
    border-top: 2px solid #f0f0f0;
    position: relative;
    border-bottom: 2px solid #f0f0f0;
    padding: 10px 0px;
}
.Collage-menu .li {
    position: relative;
    flex: 1;
    font-size: 15px;
    color: #222;
    border-right: 1px solid #f0f0f0;
    text-align: center;
}
.border_r{
    border-right:1px solid #fff!important;
}
.active .xian{
	width: 40%;
	height: 1px;
	background-color: #29d5d0;
	position: absolute;
	left: 50%;
	margin-left: -20%;
	bottom: -10px;
}
.Collage-menu .active{
	color: #29d5d0;
}
