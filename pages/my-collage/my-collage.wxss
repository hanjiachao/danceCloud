/* 拼团导航 */
.my-release-menu {
    background: -webkit-linear-gradient(90deg, #50f1c0, #29ddd9);
    background: -o-linear-gradient(90deg, #50f1c0, #29ddd9);
    background: -moz-linear-gradient(90deg, #50f1c0, #29ddd9);
    background: linear-gradient(90deg, #50f1c0, #29ddd9);
    height: 20px;
    line-height: 20px;
    display: flex;
    padding: 10px 0px;
}
.my-release-menu .active:nth-child(1) {
    color: #fffd7e;
    /*border-right: 1px solid #fff;*/
}
.my-release-menu .li:nth-child(1), .my-release-menu .li:nth-child(3), .my-release-menu .li:nth-child(5), .my-release-menu .li:nth-child(7), .my-release-menu .li:nth-child(9) {
    float: left;
    position: relative;
    flex: 1;
    text-align: center;
    font-size: 14px;
    color: #fff;
}
.active span{
    width: 6px;
    height: 6px;
    border-radius: 3px;
    background-color: #f4ff76;
    position: absolute;
    top: 0px;
    right: 35%;
}
span {
    display: inline-block;
}
