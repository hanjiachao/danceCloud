/* pages/addressBook/addressBook.wxss */
/*地址管理*/
.null{height: 80px;line-height: 80px;color: #999;font-size: 14px;text-align: center}

.content-box{
	padding-bottom: 58px;
}
.top {
	position: fixed;
	width: 100%;
	height: 46px;
	left: 0;
	top: 0;
	background: #fff;
}

.top .p {
	margin: 0 60px;
	text-align: center;
	line-height: 46px;
	font-size: 16px;
}

.return_img {
	width: 12px;
	height: 18px;
	float: left;
	margin: 11px 0 0 15px;
}

.return_img image {
	width: 100%;
	height: 100%;
}

.list-adress .li {
	padding-top: 10px;
	background: #fff;
	margin-bottom: 6px;
}

.list-adress .li> .p {
	height: 25px;
	line-height: 25px;
	padding: 0 15px
}

.list-adress .li> .p .span {
	float: right;
}

.handless {
	height: 40px;
	background: #fff;
	position: relative;
	margin-top: 10px;
	border-top: 1px solid #eee
}

.handless .checkbox {
	position: absolute;
	height: 40px;
	line-height: 40px;
	left: 15px
}

.handless .checkbox label {
	margin-left: 25px;
}
.handless .checkbox label.active{
	color: #29d5d0;
}

.handless .checkbox>input[type="checkbox"] {
	position: absolute;
	left: 0;
	top: 11px;
	outline: none;
	-webkit-appearance: none;
	border: 1px solid #29d5d0;
	border-radius: 3px;
	height: 16px;
	width: 16px;
	margin-right: 5px
}

.handless .checkbox>input[type="checkbox"]:checked {
	/*background: url(../images/a_75_s.png) no-repeat center center;*/
	width: 16px;
	height: 16px;
	background-size: 16px 16px;
	border: 0;
}

.handless > .a {
	width: 50px;
	height: 40px;
	line-height: 40px;
	position: absolute;
	text-align: right;
	font-size: 14px
}

.handless .top {
	right: calc(17% + 100px)
}

.handless .revise {
	text-indent: 25px;
	float: right;
	width: 70px;
	height: 40px;
	line-height: 40px;
	position: relative;
}

.handless .del {
	text-indent: 25px;
	float: right;
	width: 70px;
	height: 40px;
	line-height: 40px;
	position: relative;
}

.handless image {
	width: 16px;
	height: 16px;
	position: absolute;
	top: 12px;
	left: 0
}
.handless .input{
	position: absolute;
	left: 0;
	top: 11px;
	outline: none;
	-webkit-appearance: none;
	border: 1px solid #29d5d0;
	border-radius: 3px;
	height: 16px;
	width: 16px;
	margin-right: 5px
}
.handless .active .input {
	position: absolute;
    left: 0;
    top: 11px;
    /*background: url(data:image/png;base64,) no-repeat center center;*/
    width: 16px;
    height: 16px;
    border: 0;
    background-size: 100% 100%;
	display: block;
}

