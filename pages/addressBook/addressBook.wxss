.bottom {
	width: 100%;
	height: 45px;
	background: #fff;
	position: fixed;
	bottom: 0
}

.bottom .bottomBtn {
	background: rgb(255, 160, 40);
	color: #f5f5f5;
	width: 92%;
	height: 35px;
	line-height: 35px;
	text-align: center;
	margin: 5px auto;
	border-radius: 4px
}
.rowc {
	background: #fff;
	height: 40px;
	line-height: 40px;
	border-bottom: 1px solid #eee;
	position: relative
}

.rowc label {
	margin-left: 15px
}

.rowc .arrow {
	float: right;
	margin-right: 15px;
	width: 8px;
	height: 14px;
	margin-top: 13px;
}

.rowc .clear {
	position: absolute;
	color: #999;
	right: 15px;
	top: 0
}

.checkbox {
	position: absolute;
	height: 40px;
	line-height: 40px;
	left: 15px
}

.checkbox label {
	margin-left: 25px
}

.checkbox>input[type="checkbox"] {
	position: absolute;
	left: 0;
	top: 11px;
	outline: none;
	-webkit-appearance: none;
	border: 1px solid #ccc;
	border-radius: 3px;
	height: 16px;
	width: 16px;
	margin-right: 5px
}

.checkbox>input[type="checkbox"]:checked {
	position: absolute;
	left: 0;
	/*background: url(../images/a_75_s.png) no-repeat center center;*/
	width: 16px;
	height: 16px;
	border: 0;
	background-size: 100% 100%;
}
