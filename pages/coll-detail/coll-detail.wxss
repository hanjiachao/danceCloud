/* pages/class-detail/class-detail.wxss */
/*课程详情*/
.bottomBtn{width: 100% !important;background: -webkit-linear-gradient(90deg, #50f1c0, #29ddd9);background: -o-linear-gradient(90deg, #50f1c0, #29ddd9);background: -moz-linear-gradient(90deg, #50f1c0, #29ddd9);background: linear-gradient(90deg, #50f1c0, #29ddd9);height: 35px;line-height: 35px;color: #fff !important;border-radius: 17px;font-size: 16px;}

.setting-form {
    padding: 0px 0px;
}
.lan {
    color: #29D5D0;
}
.btn_bottom{
    background: -webkit-linear-gradient(90deg, #50f1c0, #29ddd9)!important;
    background: -o-linear-gradient(90deg, #50f1c0, #29ddd9)!important;
    background: -moz-linear-gradient(90deg, #50f1c0, #29ddd9)!important;
    background: linear-gradient(90deg, #50f1c0, #29ddd9)!important;
    width:100%!important;
    color: #fff!important;
}
.setting-form .li {
    padding: 0px 15px;
    border-bottom: 1px solid #eee;
    margin-bottom: 0px;
    background-color: white;
}
.setting-form .left {
    float: left;
    line-height: 45px;
    color: #666;
}
.setting-form .left span {
    font-size: 14px;
}
.setting-form .right1 {
    float: right;
    line-height: 45px;
    color: #999;
}
.order-tu {
    width: 70px;
    height: 70px;
    margin-right: 15px;
}
.class-detail {
    border-top: 6px solid #f0f0f0;
}
.class-detail .order {
    margin-bottom: 0px;
}
.my-release .order {
    border-bottom: 1px solid #f0f0f0;
}
.my-release ul .li .left {
    float: left;
    /* width: 80px; */
}
.detail-current .ul .li .left {
    float: left;
    width: 80px;
}
.my-release .ul .li .index-right {
    float: left;
    width: calc(100% - 80px);
}
.right {
    /* float: right; */
    bottom: 17px;
}
.my-release .ul .order .right label {
    line-height: 25px;
    height: 25px;
}
.my-release .ul .li .index-right .label {
    width: 100%;
    color: #333;
    font-size: 14px;
    line-height: 25px;
    height: 25px;
    overflow:hidden;
    text-overflow:ellipsis;
    white-space:nowrap
}
.price-lan {
    color: #29D5D0;
    font-size: 16px;
    margin-right: 6px;
}
.my-release .li {
    padding: 10px 15px;
    position: relative;
    background-color: white;
}
span {
    display: inline-block;
}
.confirm-box .ul {
    padding: 0px 15px;
}
.confirm-box .ul .li:nth-child(1) {
    color: #29d5d0;
    border: 1px solid #29d5d0;
    height: 33px;
    line-height: 33px;
    border-radius: 21px;
    font-size: 16px;
}
.confirm-box .ul .li:nth-child(2) {
    float: right;
    background: -webkit-linear-gradient(90deg, #50f1c0, #29ddd9);
    background: -o-linear-gradient(90deg, #50f1c0, #29ddd9);
    background: -moz-linear-gradient(90deg, #50f1c0, #29ddd9);
    background: linear-gradient(90deg, #50f1c0, #29ddd9);
    height: 35px;
    line-height: 35px;
    color: white;
    border-radius: 21px;
    font-size: 16px;
}
.confirm-box .ul .li {
    float: left;
    width: 45%;
}
.share {
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    background: rgba(50, 50, 50, 0.5);
}
.share_p{
    margin-top:70px!important;
}
.shareBtn{
    background: url("data:image/png;base64,");
    width:60px;
    height:60px;
    border: none !important;
    outline: none !important;
    background-size: 100% 100%;
    position: absolute;
    margin-bottom:11px;
    left:50px;
    display: block;
    margin-left:-30px;
}
.shareBtn:after{
    display: none;
}
.share .withdrawC {
    width: 30%;
    background: #fff;
    left: 50%;
    top: calc(50% - 90px);
    border-radius: 6px;
    position: fixed;
    margin-left: -15%;
    padding: 20px 10px;
    text-align: center;
}
.share .withdrawC image{
    width: 40%;
    margin-bottom: 20px;
}
.share .withdrawContent {
    width: 50%;
    background: #fff;
    left: 50%;
    top: calc(50% - 90px);
    border-radius: 6px;
    position: fixed;
    margin-left: -25%;
    padding: 20px 0px;
}
.share .withdrawContent .ul .li{
    width: 50%;
    float: left;
    text-align: center;
}
.share .withdrawContent .ul .li image{
    width: 60px;
    height: 60px
}
.share .withdrawContent .ul .li p{
    color: #333;
    display: block;
    margin-top: 6px;
    font-size: 14px
}

/*课程详情*/
