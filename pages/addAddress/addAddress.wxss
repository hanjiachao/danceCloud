/*遮罩*/
.member .withdrawContent {
    width: 50%;
    background: #fff;
    left: 50%;
    top: calc(50% - 90px);
    border-radius: 6px;
    position: fixed;
    margin-left: -25%;
}
.withdrawContent .btnGroup {
    width: 80%;
    height: 60px;
    position: relative;
    margin: 0 auto;
}
.withdrawContent .btnGroup .submit {
    right: 5%;
    background: -webkit-linear-gradient(90deg, #50f1c0, #29ddd9); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(90deg, #50f1c0, #29ddd9); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(90deg, #50f1c0, #29ddd9); /* Firefox 3.6 - 15 */
    background: linear-gradient(90deg, #50f1c0, #29ddd9); /* 标准的语法（必须放在最后） */
    color: #fff;
    padding: 3px 3px;
    display: block;
    position: absolute;
    text-align: center;
    box-sizing: border-box;
    top: 15px;
    font-size: 14px;
    width: 40%;
    border-radius: 12px;
}
.submit-success {
    right: 22%;
    background: -webkit-linear-gradient(90deg, #50f1c0, #29ddd9); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(90deg, #50f1c0, #29ddd9); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(90deg, #50f1c0, #29ddd9); /* Firefox 3.6 - 15 */
    background: linear-gradient(90deg, #50f1c0, #29ddd9); /* 标准的语法（必须放在最后） */
    color: #fff;
    height: 30px;
    line-height: 30px;
    display: block;
    position: absolute;
    text-align: center;
    border: 1px solid #29d5d0;
    box-sizing: border-box;
    bottom: 0px;
    font-size: 14px;
    width: 25%;
    border-radius: 16px;
}
.withdrawContent .btnGroup .cancel {
    left: 5%;
    background: #fff;
    color: #29d5d0;
    width: 40%;
    padding: 2px 2px;
    display: block;
    position: absolute;
    text-align: center;
    border: 1px solid #29d5d0;
    box-sizing: border-box;
    top: 15px;
    font-size: 14px;
    border-radius: 12px;
}
