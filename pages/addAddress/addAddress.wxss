.handless {
	height: 40px;
	background: #fff;
	position: relative;
	margin-top: 10px;
	border-top: 1px solid #eee
}

.handless .checkbox {
	position: absolute;
	height: 40px;
	line-height: 40px;
	left: 15px
}

.handless .checkbox label {
	margin-left: 25px;
	color: #29d5d0;
}

.handless .input[type="checkbox"] {
	position: absolute;
	left: 0;
	top: 11px;
	outline: none;
	-webkit-appearance: none;
	border: 1px solid #29d5d0;
	border-radius: 3px;
	height: 16px;
	width: 16px;
	margin-right: 5px
}

.handless .input[type="checkbox"]:checked {
	background: url(../images/a_75_s.png) no-repeat center center;
	width: 16px;
	height: 16px;
	background-size: 16px 16px;
	border: 0;
}

.handless>a {
	width: 50px;
	height: 40px;
	line-height: 40px;
	position: absolute;
	text-align: right;
	font-size: 14px
}

.handless .top {
	right: calc(17% + 100px)
}

.handless .revise {
	right: calc(10% + 50px)
}

.handless .del {
	right: 15px
}

.handless i {
	width: 16px;
	height: 16px;
	position: absolute;
	top: 12px;
	left: 0
}

